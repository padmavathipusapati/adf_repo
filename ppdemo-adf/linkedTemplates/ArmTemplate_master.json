{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ppdemo-adf"
		},
		"ABLB_ppblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABLB_ppblob'"
		},
		"ADLS_ppadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_ppadls'"
		},
		"ASQL_ppazsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ASQL_ppazsql'"
		},
		"ADLS_ppadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ppest2adlsgen2.dfs.core.windows.net"
		},
		"HTTP_Lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ppdemo-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABLB_ppblob_connectionString": {
						"value": "[parameters('ABLB_ppblob_connectionString')]"
					},
					"ADLS_ppadls_accountKey": {
						"value": "[parameters('ADLS_ppadls_accountKey')]"
					},
					"ASQL_ppazsql_connectionString": {
						"value": "[parameters('ASQL_ppazsql_connectionString')]"
					},
					"ADLS_ppadls_properties_typeProperties_url": {
						"value": "[parameters('ADLS_ppadls_properties_typeProperties_url')]"
					},
					"HTTP_Lego_properties_typeProperties_url": {
						"value": "[parameters('HTTP_Lego_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}